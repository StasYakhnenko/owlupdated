@model string
@using Common.Resources
@using OwlTesting.ViewModels
@Html.ValidationSummary()
<form asp-action="DeleteUser" role="form">
    @Html.Partial("_ModalHeader", new ModalHeader { Heading = General.UserRemoving })

    <div class="modal-body form-horizontal">
       @General.DeleteConfirmation @Model?
    </div>
    @Html.Partial("_ModalFooter", new ModalFooter { SubmitButtonText = General.Remove })
</form>

